[user]
	name = Bernie Telles
	email = btelles@gmail.com
[color]
	ui = auto
[color "branch"]
	current = blue reverse
	local = blue
	remote = green
[color "diff"]
	meta = blue bold
	frag = magenta bold
	old = red bold
	new = green
[color "status"]
	added = blue
	changed = green
	untracked = cyan
[alias]
	st = status
	ci = commit
	br = branch
	co = checkout
	df = diff
	lg = log -p
	lol = log --graph --decorate --pretty=oneline --abbrev-commit
	lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
	ls = ls-files
[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
	ui = true
[core]
  editor = vim
