[user]
	name = Bernardo Telles
	email = btelles@google.com
[color]
	ui = auto
[color "branch"]
	current = blue reverse
	local = blue
	remote = green
[color "diff"]
	meta = blue bold
	frag = magenta bold
	old = red bold
	new = green
[color "status"]
	added = blue
	changed = green
	untracked = cyan
[alias]
	st = status
	ci = commit
	br = branch
	co = checkout
  f = fetch
  p = pull
	df = diff
	lg = log -p
	lol = log --graph --decorate --pretty=oneline --abbrev-commit
	lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
	cb = rev-parse --abbrev-ref HEAD
  pcb = ! sh -c '(git push origin $(git cb))'
  ls = ls-files
[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
	ui = true
[core]
  editor = vim
[branch "master"]
  mergeoptions = --no-ff
[giggle]
	main-window-maximized = true
	main-window-geometry = 700x550+1+53
	history-view-vpane-position = 312
	main-window-view = HistoryView
	file-view-hpane-position = 388
	file-view-vpane-position = 982
[push]
	default = simple
[diff]
  renameLimit = 10000
[gui]
	fontui = -family \"DejaVu Sans\" -size 12 -weight normal -slant roman -underline 0 -overstrike 0
	fontdiff = -family \"DejaVu Sans Mono\" -size 12 -weight normal -slant roman -underline 0 -overstrike 0
